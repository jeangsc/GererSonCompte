<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EditTextErrorChecker">
        <attr name="filter">
            <flag name="filterNone"        value="0" />
            <flag name="filterNotEmpty"    value="1" />
            <flag name="filterNotZero"     value="2" />
            <flag name="filterDateExists"  value="4" />
        </attr>
        <attr name="textError" format="string" />
    </declare-styleable>
    <declare-styleable name="SeekBarRange">
        <attr name="minVal" format="float" />
        <attr name="maxVal" format="float" />
        <attr name="posLeft" format="float" />
        <attr name="posRight" format="float" />
        <attr name="nSteps" format="integer" />
    </declare-styleable>
    <declare-styleable name="SeekBarRangeValues">
        <attr name="seekBarMinVal" format="float" />
        <attr name="seekBarMaxVal" format="float" />
        <attr name="seekBarPosLeft" format="float" />
        <attr name="seekBarPosRight" format="float" />
    </declare-styleable>
    <declare-styleable name="EditTextDate">
        <attr name="editDateSize" format="integer" />
    </declare-styleable>
</resources>
